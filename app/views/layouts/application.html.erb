    <label>Boards</label>
  </a>
  <a class="item" href="/pins">
    <!-- <img src="img/images/fi-mail.svg" > -->
    <label>Pins</label>
  </a>
  <a class="item" href="/about">
    <!-- <img src="img/images/fi-mail.svg" > -->
    <label>About</label>
  </a>
  
</div>
<div class="content">
    <%= yield %>
    </div>
    <%= javascript_include_tag "application" %>
  </body>
</html>